042DDE DD 7E 00    0031*       ld a,(ix) ; tile number
042DE1 FD BE 09    0032*       cp (iy+maze_null_tile) ; is it the null tile?
042DE4 28 19       0033*       jr z,@next_tile ; yes, so don't draw anything
